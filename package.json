{
  "name": "FootballApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod:install": "cd ios && pod install && cd ..",
    "pod:deintegrate": "cd ios && pod deintegrate && cd ..",
    "prepare": "husky install",
    "detox-android:debug": "detox test --configuration android.emu.debug",
    "detox-android:release": "detox test --configuration android.emu.release",
    "detox-ios:debug": "detox test --configuration ios.sim.debug",
    "detox-ios:release": "detox test --configuration ios.sim.release"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.3",
    "axios": "^0.27.2",
    "detox": "^20.0.3",
    "jest-config": "^29.3.1",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-asset": "^2.0.1",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-shake": "^5.1.1",
    "react-native-vector-icons": "^9.2.0",
    "react-query": "^3.39.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.5.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "29.2.3",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  }
}
